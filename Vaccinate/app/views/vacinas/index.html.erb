<p id="notice"><%= notice %></p>

<h1>Vacinas</h1>

<table>
  <thead>
    <tr>
      <th>Idade</th>
      <th>Vacina</th>
      <th>Doenca protecao</th>
      <th>Dose</th>
      <th>Dose qtd</th>
      <th>Via administacao</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @vacinas.each do |vacina| %>
      <tr>
        <td><%= vacina.idade %></td>
        <td><%= vacina.vacina %></td>
        <td><%= vacina.doenca_protecao %></td>
        <td><%= vacina.dose %></td>
        <td><%= vacina.dose_qtd %></td>
        <td><%= vacina.via_administacao %></td>
        <td><%= link_to 'Show', vacina %></td>
        <td><%= link_to 'Edit', edit_vacina_path(vacina) %></td>
        <td><%= link_to 'Destroy', vacina, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Vacina', new_vacina_path %>
